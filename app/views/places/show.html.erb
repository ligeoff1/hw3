<h1> <%=@place.name%></h1>

<ul>
    <% for post in @place.posts %>
        <li>
            <p>
                <%=  post.title%>
            </p>

            <p>
                <%=  post.description%>
            </p>

            <p>
                <%=  post.posted_on%>
            </p>
        </li>
    <%end%>

</ul>

<p>
<a href="/places/new?place_id=<%= @place.id %>"> Add place </a>
</p>

<p>
<a href="/posts/new?place_id=<%= @place.id %>"> Add post </a>
</p>

<p>
<a href="/places">Back to all places</a>
</p>


